@using Models
@model Type

@{
    ViewBag.Title = "Admin page";
    Layout = "_Layout";
    var action = "/Admin/Create" + Model.Name;
}

<div>
    <a asp-controller="Admin" asp-action="Models" asp-route-name="@Model.Name">Back</a>
</div>

<form id="create" method="post" action="@action"  enctype="multipart/form-data">
    @Html.GetHtml(CreateHelper.getCreateHtml(Model))
    <div>
        <button id="btn">Create</button>
    </div>
</form>
